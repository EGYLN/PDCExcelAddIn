using System.Runtime.InteropServices;
namespace BBS.ST.BHC.BSP.PDC.ExcelClient.VBA
{
    /// <summary>
    /// Result status of a performed action for the calling VBA
    /// </summary>
    [ComVisible(true)]
  public struct VBAActionStatus
  {
    /// <summary>
    /// 0 if no messages, pdc log-level otherwise 1: Info, 3:Warning, 4:Error
    /// </summary>
    public int Status;
    public string[] ExceptionMessage;
    public string[] Messages;
  }
}
